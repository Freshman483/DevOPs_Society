            <TextView
                android:id="@+id/activityTitleDevopsDescriptionText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="44dp"
                android:text="Participate"
                android:textColor="@color/purple_500"
                android:textSize="22sp"
                app:layout_constraintStart_toEndOf="@+id/circleImageViewForum"
                app:layout_constraintTop_toBottomOf="@+id/activityTitleForum" />


            <ImageView
                android:id="@+id/lockOrOpen_devops_Forum"
